{
  "name": "@katorymnd/pawapay-node-sdk",
  "version": "2.6.2",
  "description": "Official Premium Node.js SDK by Katorymnd Web Solutions for PawaPay mobile money API integration.",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "data",
    "scripts",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:coverage": "jest --coverage",
    "dev": "nodemon --watch example --watch src --ext js,ejs,json --legacy-watch example/server.js",
    "start": "node example/server.js",
    "lint": "eslint src/**/*.js example/**/*.js tests/**/*.js",
    "lint:fix": "eslint src/**/*.js example/**/*.js tests/**/*.js --fix",
    "clean": "pnpm store prune",
    "clean:dist": "rimraf dist",
    "update": "pnpm update --latest",
    "dev:vite": "vite",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check-master-keys": "node scripts/checkMasterKeys.js",
    "build": "pnpm check-master-keys && vite build",
    "build:vm": "node scripts/vm-encode-bytecode.js",
    "vm:test": "node scripts/test-vm.js",
    "build:secure": "pnpm check-master-keys && node scripts/obfuscate.js",
    "build:full": "pnpm clean:dist && pnpm build:vm && pnpm build:secure",
    "test:install": "pnpm build:secure && cd dist && node scripts/install-sdk.js && echo 'âœ… Test Complete: Wizard ran successfully'",
    "postinstall": "node scripts/install-sdk.js",
    "publish:secure": "pnpm build:full && echo 'ðŸ” Verifying Installer...' && cd dist && node scripts/install-sdk.js && echo 'ðŸ§¹ Cleaning Dev Artifacts...' && rimraf src/utils/vm/*.bin src/utils/vm/*.json && echo 'ðŸš€ Publishing...' && npm publish --access public"
  },
  "keywords": [
    "katorymnd",
    "pawapay",
    "mobile-money",
    "payments",
    "africa",
    "fintech",
    "sdk",
    "nodejs",
    "premium"
  ],
  "author": {
    "name": "Katorymnd Web Solutions",
    "email": "support@katorymnd.com",
    "url": "https://katorymnd.com"
  },
  "maintainers": [
    {
      "name": "Kato Raymond",
      "email": "support@katorymnd.com"
    },
    {
      "name": "Kato Raymond",
      "email": "katorymnd@gmail.com"
    }
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^1.12.2",
    "dotenv": "^16.6.1",
    "joi": "^18.0.1",
    "libphonenumber-js": "^1.12.24",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "bootstrap": "^5.3.3",
    "connect-livereload": "^0.6.1",
    "ejs": "^3.1.10",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "express": "^4.21.2",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.7.0",
    "livereload": "^0.10.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.2",
    "supertest": "^6.3.4",
    "vite": "^7.1.12",
    "vite-plugin-ejs": "^1.7.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.19.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/katorymnd/pawapay-node-sdk.git"
  },
  "homepage": "https://katorymnd.com/pawapay-payment-sdk/nodejs",
  "bugs": {
    "url": "https://github.com/katorymnd/pawapay-node-sdk/issues"
  }
}